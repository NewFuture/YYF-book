{"docs":[{"urls":["database/","database/db.html"],"_id":"word__3306","_rev":"2-5f8d3fc43e613403e84eef301687d694","_revisions":{"start":2,"ids":["5f8d3fc43e613403e84eef301687d694","936fa229664692f25c82bcef6207dc16"]}},{"urls":["database/","database/db.html"],"_id":"word__CHARSET","_rev":"2-d28b0a5048944efa771f21b9e15dcbb3","_revisions":{"start":2,"ids":["d28b0a5048944efa771f21b9e15dcbb3","5f1c8a1e2f70074755235c55d0908054"]}},{"urls":["database/","database/db.html"],"_id":"word__PORT","_rev":"2-69fc408fdee81fdc41b9e4409e2328bf","_revisions":{"start":2,"ids":["69fc408fdee81fdc41b9e4409e2328bf","d1b189309deabdea63d86a3efe533676"]}},{"urls":["setup/yyf-in-server.html","database/","database/db.html"],"_id":"word__ROOT","_rev":"3-63fbd4a1698585047f98ab448fa25995","_revisions":{"start":3,"ids":["63fbd4a1698585047f98ab448fa25995","ba8b864060733cf9443691648f5d2737","a41c1338736a2fa0e03719639672331b"]}},{"urls":["database/","database/db.html"],"_id":"word__UTF8","_rev":"2-4a780f1d9be5103589abf37733e09b8b","_revisions":{"start":2,"ids":["4a780f1d9be5103589abf37733e09b8b","21441c2f2f27628025fb609de4e75ee1"]}}]}
