{"docs":[{"urls":["database/","database/db.html"],"_id":"word__3306","_rev":"2-8d05676acc667e0a2213674d4112285e","_revisions":{"start":2,"ids":["8d05676acc667e0a2213674d4112285e","17d82584820a0dc580fa6c9c8224b635"]}},{"urls":["database/","database/db.html"],"_id":"word__CHARSET","_rev":"2-cbf8a5c9a2eb3bcad562025d6855170b","_revisions":{"start":2,"ids":["cbf8a5c9a2eb3bcad562025d6855170b","24c2713f79387e40137e0b45001a4a88"]}},{"urls":["database/","database/db.html"],"_id":"word__PORT","_rev":"2-6b3197992ada32affc318003b7b57207","_revisions":{"start":2,"ids":["6b3197992ada32affc318003b7b57207","4ec85a2200eb9df055cd7e2e1a97e5f1"]}},{"urls":["setup/yyf-in-server.html","database/","database/db.html"],"_id":"word__ROOT","_rev":"3-ff488839cbde0f9825839d409915b911","_revisions":{"start":3,"ids":["ff488839cbde0f9825839d409915b911","51f82a471b8d782b40a97e84e39c5050","1f5636e2367ec4442f2a91e5c475e51b"]}},{"urls":["database/","database/db.html"],"_id":"word__UTF8","_rev":"2-a4ab17c0d183a9ec7ad7adc62e34a2ef","_revisions":{"start":2,"ids":["a4ab17c0d183a9ec7ad7adc62e34a2ef","9123ed85e75518f2215726973deb0237"]}}]}
