tart":2,"ids":["99bd85aa34224e42e3ae1f3d1c8a6ae1","f1b7ad58968ce4d1f80a7f5c2dc1a5ca"]}},{"urls":["setup/yyf-in-windows.html"],"_id":"word__拖拽","_rev":"1-ae62079f29ad1ba372391e7f6b19a06f"},{"urls":["about/","setup/yyf-in-linux.html"],"_id":"word__换成","_rev":"2-5aba1ea20de0c6a4966c293396dfc5c0","_revisions":{"start":2,"ids":["5aba1ea20de0c6a4966c293396dfc5c0","4f2e2cfee3f4a5ea10fac36abe3b795c"]}},{"urls":["setup/yyf-in-windows.html"],"_id":"word__放于","_rev":"1-e2684287b7dd9ea936ec494696b6ea0d"},{"urls":["setup/yyf-in-windows.html"],"_id":"word__放入","_rev":"1-888b2e06d42b9e88706220f35a719ae1"},{"urls":["setup/yyf-in-linux.html"],"_id":"word__有所","_rev":"1-1b3353ad6fbfef3d2ea7056554ab3ce2"},{"urls":["setup/yyf-in-linux.html"],"_id":"word__有所不同","_rev":"1-a1874ecba0c52f5d8197bc1d6fb88c23"},{"urls":["setup/yyf-in-windows.html","setup/yyf-in-linux.html"],"_id":"word__步骤","_rev":"2-59e6b5d2396957e92d8c1a3e96316d1d","_revisions":{"start":2,"ids":["59e6b5d2396957e92d8c1a3e96316d1d","0a2442dd8e3027ba06883e1752a996b1"]}},{"urls":["./","setup/develop.html","setup/vm-in-linux.html","setup/yyf-in-linux.html"],"_id":"word__源码","_rev":"4-349643a060919a43c24df80225dec92f","_revisions":{"start":4,"ids":["349643a060919a43c24df80225dec92f","d53f941b3baf3d05edb86f2b63a60ca6","dc146df9d5d6214e3622ba9b373e4631","a269de4a64cc2a7a3c44b5b82a76de99"]}},{"urls":["./","setup/yyf-in-windows.html"],"_id":"word__版","_rev":"2-8dfed19bbdb4be480568d835bed675cd","_revisions":{"start":2,"ids":["8dfed19bbdb4be480568d835bed675cd","ed82d6d34c0a7f36e5df1124b75de873"]}},{"urls":["setup/yyf-in-linux.html"],"_id":"word__版本号","_rev":"1-503e9152b0aeca7de9030e58d6ff6e95"},{"urls":["setup/yyf-in-linux.html"],"_id":"word__稳定版","_rev":"1-e9ef6192a8d12e64408b5595f066908b"},{"urls":["setup/yyf-in-windows.html"],"_id":"word__系统目录","_rev":"1-cb6c2a225f8e8f092975bd5843c3ceb3"},{"urls":["setup/yyf-in-linux.html"],"_id":"word__退出","_rev":"1-193e4f9449578a899c3f320507642736"}]}
