{"docs":[{"urls":["database/model.html"],"_id":"word__UNICODE","_rev":"1-82295260d40c605a14a92695e7b96efe"},{"urls":["architecture/folder-structure.html","architecture/mvc.html","database/","database/model.html"],"_id":"word__数据模型","_rev":"4-be26f15ac2a52bbb55439cc412cb85b8","_revisions":{"start":4,"ids":["be26f15ac2a52bbb55439cc412cb85b8","8af2225baa98eff6aa95a2af26d078ce","328703dccf7cf0adf56ce8326f8e8b0c","c1f9152c1b721f47407a7b5bcb68ed96"]}},{"urls":["architecture/folder-structure.html","architecture/mvc.html","database/","database/model.html"],"_id":"word__模型","_rev":"4-1e0fb0fada19e09cb12258c3b1982873","_revisions":{"start":4,"ids":["1e0fb0fada19e09cb12258c3b1982873","fe4a03aa4367d3a040ede778c236741d","064b615cfc54cbd8bd6856632e9e6f95","6a19ed5f70af2edec5b3eed00e031c23"]}},{"urls":["database/model.html"],"_id":"word__编码方式","_rev":"1-f3c7f4ec870606a9ed4ca532d0a20642"}]}
