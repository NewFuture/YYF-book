isions":{"start":2,"ids":["217d5e1e0a541469b2bfa233ab524be0","9e0cae2c51eb2ee6141a0f406d79b732"]}},{"urls":["setup/yyf-in-linux.html","architecture/mvc.html"],"_id":"word__已","_rev":"2-5dfeec96e53e4a3ec80b7d818557f8cf","_revisions":{"start":2,"ids":["5dfeec96e53e4a3ec80b7d818557f8cf","c652d5581a61d4a6509dc846f03a8a2d"]}},{"urls":["architecture/rest.html"],"_id":"word__常见","_rev":"1-2d0065a664a4f011244a865ee44f6e9b"},{"urls":["about/","architecture/mvc.html"],"_id":"word__数据格式","_rev":"2-cd812d64d751fc101f298fdf45d54fff","_revisions":{"start":2,"ids":["cd812d64d751fc101f298fdf45d54fff","3c588285328a2714783027a2618d489b"]}},{"urls":["architecture/rest.html"],"_id":"word__新增","_rev":"1-e2f27759ebd3b5530735ada6f3b5da1c"},{"urls":["architecture/rest.html"],"_id":"word__标识","_rev":"1-96dcac59517be7965ba0efd4745b9f06"},{"urls":["configure/config.html"],"_id":"word__此值","_rev":"1-6c58f706a0db1638c9f1b96a4e227bd1"},{"urls":["architecture/mvc.html"],"_id":"word__纯","_rev":"1-604556ca15a8c3fd409192c6b625fb1e"},{"urls":["architecture/rest.html"],"_id":"word__组","_rev":"1-d9b50d86a0d1daea0d945a295d88592b"},{"urls":["./","architecture/mvc.html"],"_id":"word__而来","_rev":"2-edd443ed17428dd6cf81c7de8edeafde","_revisions":{"start":2,"ids":["edd443ed17428dd6cf81c7de8edeafde","2359bbb3ed3e50d21fd8b6173522f133"]}},{"urls":["architecture/folder-structure.html","architecture/mvc.html"],"_id":"word__视图","_rev":"2-e101430b0ed181bdb6ea484321d60248","_revisions":{"start":2,"ids":["e101430b0ed181bdb6ea484321d60248","86d6314fb3c0ab93dc68e7f9d3d8b8ec"]}},{"urls":["architecture/mvc.html"],"_id":"word__语法","_rev":"1-51ae12129ce0594b372576c62462565a"},{"urls":["architecture/rest.html"],"_id":"word__贡献","_rev":"1-aa8376e82ed9730d8e0e1aece750fb16"},{"urls":["architecture/rest.html"],"_id":"word__贡献者","_rev":"1-81ea584250898adebd0df63afc57a61c"},{"urls":["./","setup/develop.html","architecture/mvc.html"],"_id":"word__集成","_rev":"3-f1e440503eef4cc4f5d74e09fe08e828","_revisions":{"start":3,"ids":["f1e44