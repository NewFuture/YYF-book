:"1-3899c1dff5e63f55be3ab1756df529bd"},{"urls":["./","about/","setup/yyf-in-windows.html","setup/yyf-in-linux.html","architecture/folder-structure.html"],"_id":"word__输入","_rev":"5-0f24059b7c9012c801990546c9e48563","_revisions":{"start":5,"ids":["0f24059b7c9012c801990546c9e48563","b9d01bb53f0d730c1f4fecb536f05929","e4c6d354580203466221e7b734db766f","e51292f80a51d9957b046385b563de53","6f2a5a46a55e51006d29747e1ea3f0a2"]}},{"urls":["./","architecture/folder-structure.html"],"_id":"word__驱动","_rev":"2-89b45b34606853f222412607fe1a139f","_revisions":{"start":2,"ids":["89b45b34606853f222412607fe1a139f","dad4cf33d701fa35a822ed9bc29b07ee"]}}]}
