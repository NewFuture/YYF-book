:"1-f41abe590422736a38ae0e01d2086b4a"},{"urls":["./","about/","setup/yyf-in-windows.html","setup/yyf-in-linux.html","architecture/folder-structure.html"],"_id":"word__输入","_rev":"5-4c6895511ead23b71646056501ff3ef0","_revisions":{"start":5,"ids":["4c6895511ead23b71646056501ff3ef0","02cda32760d3be5a230e0e668e4ca46c","b89afc991106bb021ff7ce96a2a20aec","dd52cdf304da04071e1aca88065bbe19","e7c1f6e7d4cf72fb3e70fabf911ad564"]}},{"urls":["./","architecture/folder-structure.html"],"_id":"word__驱动","_rev":"2-840b92ec9110c9b164385278f619fb98","_revisions":{"start":2,"ids":["840b92ec9110c9b164385278f619fb98","cdc4be8db79fbc1afd81d57f300f398b"]}}]}
