:"1-849fed97402f64e037bc336078be9618"},{"urls":["./","about/","setup/yyf-in-windows.html","setup/yyf-in-linux.html","architecture/folder-structure.html"],"_id":"word__输入","_rev":"5-39ea16c2bcf8cd3c13d9c1b2d8cf50b7","_revisions":{"start":5,"ids":["39ea16c2bcf8cd3c13d9c1b2d8cf50b7","0d72c42052cc7698fe1656948c66a29a","14953556736264b35dea610ee729d09e","95d097970ab56682e967326f1d85f403","66e8e813165f2ee622fc5f6dc833db8f"]}},{"urls":["./","architecture/folder-structure.html"],"_id":"word__驱动","_rev":"2-b9af95a04c819d9799852f6a5f9055ee","_revisions":{"start":2,"ids":["b9af95a04c819d9799852f6a5f9055ee","7f800d53f38af8b25a567326c40e5e06"]}}]}
