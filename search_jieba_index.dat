{"docs":[{"urls":["./","setup/develop.html","database/","database/orm.html","storage/cache.html","storage/kv.html"],"_id":"word__一致","_rev":"6-66bbbb0c4b544fec05a54a98eb600803","_revisions":{"start":6,"ids":["66bbbb0c4b544fec05a54a98eb600803","1bbd6e80b984953a5eb31f7de46637dd","01382515e7f98a8afefafacbb18ed241","e7227cec30622c48e93c92e5a9087bb2","1f42a2fbc806e874b1a52a9960042b23","a7969762763daa80820063a778aee78c"]}},{"urls":["configure/config.html","database/model.html","database/orm.html","database/database.html","debug/logger.html","storage/cache.html","storage/kv.html"],"_id":"word__字符串","_rev":"7-e61232ddc5634141f7d61d512e6b0947","_revisions":{"start":7,"ids":["e61232ddc5634141f7d61d512e6b0947","1876c07648783bb71e050f0411ba491c","65fca2d06f44e35c3268f4c45bc4acad","2b7c4eb8aa83f664862436effe887a76","ce43d625574c4143dcbc9081dcc65a11","88f219c42765b6a0a1ef56165de82936","f423bb33b15bdc28dcba0dcdd96b04c5"]}},{"urls":["architecture/folder-structure.html","storage/","storage/cache.html","storage/kv.html"],"_id":"word__存储","_rev":"4-7ff6a6f56a5c464c0ac7810b37b62d22","_revisions":{"start":4,"ids":["7ff6a6f56a5c464c0ac7810b37b62d22","b8b553f58c096de97b60351b93a2e915","bb6c4125bb165ea74d5070e14ac7a51d","4304ac99ca2fe1a5be6658c53ee3016e"]}},{"urls":["./","about/","setup/yyf-in-windows.html","setup/yyf-in-linux.html","configure/config.html","database/db.html","database/model.html","database/orm.html","debug/logger.html","storage/","storage/cache.html","storage/kv.html"],"_id":"word__快速","_rev":"12-bbc5019f5ae8df4aa2c4be7d0816c22e","_revisions":{"start":12,"ids":["bbc5019f5ae8df4aa2c4be7d0816c22e","8912ab21409c7086f1a5eafecd2b4e6c","950eacce1abbcba1bc21359bd6b00ddf","2919cb13f36fbe2d101083f3e5adad28","97dc28e8e6dba6e3cd1c866f67d155fe","a6f54cd1488adcf1e8f19e818488f71f","e433d605f70b4eb84888bc8d5c6f705e","e1104a5341c4abaaaca8e93b05fe8b21","22e5c6f46be31db1f5ac20ed94321e03","703dff0ce1290b4760cf2f43a9697113","a6580f6d17088c22fe3ded28de4e2176","fd84a3567b7eb57f01dc2253a4af2d21"