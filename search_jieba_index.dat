base/model.html"],"_id":"word__心里","_rev":"1-08534688f9f33678e79f9bd522fe289f"},{"urls":["setup/develop.html","database/db.html"],"_id":"word__新手","_rev":"2-4e4253a2304b624443cbcd3c522b712b","_revisions":{"start":2,"ids":["4e4253a2304b624443cbcd3c522b712b","c1677f75c6d670eabe20bb921253a553"]}},{"urls":["database/model.html"],"_id":"word__易读","_rev":"1-40e9ed0dc33d159b3311f2dc3a9034cb"},{"urls":["database/model.html"],"_id":"word__易读性","_rev":"1-f5d6d67f836eda0305b8e29a4b4916a4"},{"urls":["database/model.html"],"_id":"word__最终","_rev":"1-2fcaa1e92c52be52651e2f90d769b79f"},{"urls":["database/model.html"],"_id":"word__本质","_rev":"1-b6fa9c6f007955b0dbf031600a9772d1"},{"urls":["database/model.html"],"_id":"word__清晰","_rev":"1-7718014d91b3dea07dfd98e285a99eda"},{"urls":["architecture/standard.html","database/db.html"],"_id":"word__私有","_rev":"2-3162e29fc3803d1d0e09f56271b26649","_revisions":{"start":2,"ids":["3162e29fc3803d1d0e09f56271b26649","fe13691fc82c495730b6b1b06cf4d8d2"]}},{"urls":["database/model.html"],"_id":"word__空串","_rev":"1-2e7e8111125ea64e0f56069a21d804b5"},{"urls":["database/model.html"],"_id":"word__自行","_rev":"1-968f8a23c9ab446b2f52fd3c3b0fdc2f"},{"urls":["database/model.html"],"_id":"word__若干","_rev":"1-b1f218416032b3e808959e6d650000c3"},{"urls":["database/model.html"],"_id":"word__表面","_rev":"1-3640c9952ab2fd333354bd28d5bc5e98"},{"urls":["database/model.html"],"_id":"word__话","_rev":"1-cee9333273bb9d14660336afeea20b80"},{"urls":["database/model.html"],"_id":"word__赖于","_rev":"1-890fadd2e318b70deb62d1b735b07436"},{"urls":["database/model.html"],"_id":"word__这个","_rev":"1-0062a19a5ddf3c2024f632754f8f2c4e"},{"urls":["database/model.html"],"_id":"word__这里","_rev":"1-cde5f41fc05fa5dabd4ab03428e0ce63"},{"urls":["database/model.html"],"_id":"word__选出","_rev":"1-4dcb63f66fb619eca5c109fe30a4989e"},{"urls":["database/model.html"],"_id":"word__里面","_rev":"1-6ff40f0918b04f5a64d75c90de5d96dc"},{"urls":["database/model.html"],"_id":"word__重复使用","_rev":"1-4b828fb197c