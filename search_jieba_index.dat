d":"word__定为","_rev":"1-a91e6f45ed6825079e9d852beeba4b6d"},{"urls":["database/db.html"],"_id":"word__对同","_rev":"1-355fd60280d64ea2279e0e8be73bc554"},{"urls":["database/db.html"],"_id":"word__成","_rev":"1-c631f6584a710675e5dd37bca68f26c1"},{"urls":["database/db.html"],"_id":"word__无则","_rev":"1-dfa88820308e2e5b64363460d5077e2c"},{"urls":["database/db.html"],"_id":"word__时选填","_rev":"1-d69656cccc35893bf901ee3122555ac7"},{"urls":["database/db.html"],"_id":"word__正在","_rev":"1-4a6a99471f197677a392f9e91b0a7c2d"},{"urls":["database/db.html"],"_id":"word__每次","_rev":"1-2503ec9c5693e2657b8ccb671be03a98"},{"urls":["database/db.html"],"_id":"word__潜在","_rev":"1-508fd2ec18e6c27b2f1f9903bb6ba561"},{"urls":["architecture/lifecircle.html","database/db.html"],"_id":"word__生命","_rev":"2-926399894586fcd4bde1c58fd33ebe86","_revisions":{"start":2,"ids":["926399894586fcd4bde1c58fd33ebe86","68231cd89c1debdb45d1e1283e22fe20"]}},{"urls":["architecture/lifecircle.html","database/db.html"],"_id":"word__生命周期","_rev":"2-c4beb2d5a0a157e82744d1c2ced93c31","_revisions":{"start":2,"ids":["c4beb2d5a0a157e82744d1c2ced93c31","3211e349f242558d761e623c4921f372"]}},{"urls":["database/db.html"],"_id":"word__确实","_rev":"1-86cd088bd1546c5095045657984d4337"},{"urls":["database/db.html"],"_id":"word__类会","_rev":"1-4f396e29fb90a0b365c82d7c6688fbe0"},{"urls":["database/db.html"],"_id":"word__规则","_rev":"1-db8ce2eea1c3346e4bea95fa09c07302"},{"urls":["database/db.html"],"_id":"word__这么","_rev":"1-f5a3daf0f8e788923364442725b9afe4"},{"urls":["architecture/standard.html","database/db.html"],"_id":"word__这些","_rev":"2-55b0558abc8414e327728e332037f2d1","_revisions":{"start":2,"ids":["55b0558abc8414e327728e332037f2d1","6f4d410ab10fef5422ede95f67c6b580"]}},{"urls":["database/db.html"],"_id":"word__风险","_rev":"1-812ec5bd467c077c45bc7df3537cff70"}]}
