ord__而来","_rev":"2-a2226c1dc14a7039144ccc3d0497ba58","_revisions":{"start":2,"ids":["a2226c1dc14a7039144ccc3d0497ba58","96aca56a37eb4d405b7330e4b4903419"]}},{"urls":["architecture/folder-structure.html","architecture/mvc.html"],"_id":"word__视图","_rev":"2-34fc3413314305e0e5139998980b5323","_revisions":{"start":2,"ids":["34fc3413314305e0e5139998980b5323","c248fc2b96140f3eba45e68c4032eeab"]}},{"urls":["architecture/mvc.html"],"_id":"word__语法","_rev":"1-c2130e2678a1018e269e940f57a27c0e"},{"urls":["configure/config.html"],"_id":"word__转为","_rev":"1-2d5e7cb075bc56949e5e6f90b67257cd"},{"urls":["./","setup/develop.html","architecture/mvc.html"],"_id":"word__集成","_rev":"3-1f8994f6a557e2a7a97da8a8bdd6be9d","_revisions":{"start":3,"ids":["1f8994f6a557e2a7a97da8a8bdd6be9d","b1c1fd48aea6b95ceb9d0d725f2f0ded","420af57a984e246f9d09765fd3c004ee"]}},{"urls":["configure/config.html"],"_id":"word__项","_rev":"1-c68edd69aea5a7c21e5229cdfc4e1c9c"},{"urls":["configure/config.html"],"_id":"word__项如","_rev":"1-695c1a39aecee0808f44bf47c2c37625"}]}
