":["configure/"],"_id":"word__实时","_rev":"1-ac9c0cbda6fa9b6047a0ef0bcc86a153"},{"urls":["setup/yyf-in-linux.html","architecture/standard.html","architecture/lifecircle.html"],"_id":"word__工作","_rev":"3-2f706d8ac61709eeb5f8bde47371a0d1","_revisions":{"start":3,"ids":["2f706d8ac61709eeb5f8bde47371a0d1","f317fccf5af751a6d41855d31c1a7734","c439580cc9d96dcec09de94aa922588c"]}},{"urls":["architecture/standard.html","configure/"],"_id":"word__常量","_rev":"2-7a9c35d55612660c2bd36f096b5b205d","_revisions":{"start":2,"ids":["7a9c35d55612660c2bd36f096b5b205d","4c2ffb9db0012aadfcb3efdebc4625cc"]}},{"urls":["architecture/folder-structure.html","configure/"],"_id":"word__放置","_rev":"2-9e7be7f0ee5bf2988cd9f8ed75626314","_revisions":{"start":2,"ids":["9e7be7f0ee5bf2988cd9f8ed75626314","4c7df6d5cf6f091ba93da69cfc5815aa"]}},{"urls":["configure/"],"_id":"word__末","_rev":"1-ac828b655134dc0823d1cc955d0351bf"},{"urls":["configure/"],"_id":"word__样例","_rev":"1-5024a7e39dc824419aca21812ec91585"},{"urls":["configure/config.html"],"_id":"word__此值","_rev":"1-b41e8965085c2940d5766fba9da5bed1"},{"urls":["controller/rest.html"],"_id":"word__父级","_rev":"1-700fc1b92857966aefe449f93bfa1f16"},{"urls":["./","configure/"],"_id":"word__特点","_rev":"2-b10fc374b9799bf2a302f3bb277f484b","_revisions":{"start":2,"ids":["b10fc374b9799bf2a302f3bb277f484b","e5265985c1412d9762bd123f363d7ead"]}},{"urls":["architecture/folder-structure.html","configure/","configure/config.html"],"_id":"word__私密","_rev":"3-7bb7d452d9e94762153995754d93875b","_revisions":{"start":3,"ids":["7bb7d452d9e94762153995754d93875b","66df0c0de75e1ff29e498bd46f2ccb28","7675c871467b605567c6719138e7cf53"]}},{"urls":["./","configure/"],"_id":"word__系统配","_rev":"2-9ab9a9935e4b2a44a0245436634bffb2","_revisions":{"start":2,"ids":["9ab9a9935e4b2a44a0245436634bffb2","e10d26a11b4abf1d13df93a3ec13df33"]}},{"urls":["./","configure/"],"_id":"word__系统配置","_rev":"2-4f836c344be2d012abf08577a873af1a","_revisions":{"start":2,"ids":["4f836c344be2d012abf08577a873a